{
    "name": "amartinezhernandez/got-api",
    "description": "Game of Thrones API",
    "type": "project",
    "license": "MIT License",
    "authors": [
        {
            "name": "Alberto Martinez",
            "email": "albmartinez89@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-json": "*",
        "monolog/monolog": "^2.3",
        "php-di/php-di": "^6.3",
        "robmorgan/phinx": "^0.12.7",
        "slim/psr7": "^1.3",
        "slim/slim": "^4.7",
      "ext-pdo": "*"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.30.1",
        "composer/package-versions-deprecated": "^1",
        "jangregor/phpstan-prophecy": "^0.8.1",
        "phpstan/extension-installer": "^1.1.0",
        "phpstan/phpstan": "^0.12.80",
        "phpunit/phpunit": "^8.0 || ^9.0"
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\Migrations\\": "migrations/",
            "App\\Seeds\\": "seeds/"
        },
        "files": ["config/constants.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "test/"
        }
    }
}
